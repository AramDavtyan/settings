// Place your key bindings in this file to overwrite the defaults
[
    {
        "key": "ctrl+shift+m",
        "command": "expand-selection-to-scope.expand",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+down",
        "command": "cursorHome",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+up",
        "command": "cursorEnd",
        "when": "editorTextFocus"
    },
    {
        "key": "alt+ctrl+up",
        "command": "editor.action.insertCursorAbove",
        "when": "editorTextFocus"
    },
    {
        "key": "alt+ctrl+down",
        "command": "editor.action.insertCursorBelow",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+right",
        "command": "cursorEndSelect",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+left",
        "command": "cursorHomeSelect",
        "when": "editorTextFocus"
    },
    {
        "key": "shift+alt+up",
        "command": "commandId",
        "when": "editorTextFocus"
    },
    {
        "key": "shift+alt+down",
        "command": "commandId",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+space",
        "command": "center-editor-window.center",
        "when": "editorTextFocus"
    },
    // {
    //     "key": "ctrl+'",
    //     "command": "expand_region",
    //     "when": "editorTextFocus"
    // },extension.selectSingleQuote
    // {
    //     "key": "ctrl+shift+'",
    //     "command": "undo_expand_region",
    //     "when": "editorTextFocus && editorHasSelection"
    // }
    {
        "key": "ctrl+'",
        "command": "extension.selectDoubleQuote",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+shift+'",
        "command": "extension.selectSingleQuote",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+shift+a",
        "command": "extension.selectInTag",
        "when": "editorTextFocus"
    },
    {
        "key": "shift+alt+left",
        "command": "cursorWordStartLeftSelect",
        "when": "editorTextFocus"
    },
    {
        "key": "shift+alt+right",
        "command": "cursorWordEndRightSelect",
        "when": "editorTextFocus"
    },
]