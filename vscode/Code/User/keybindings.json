[
    {
        "key": "ctrl+shift+m",
        "command": "expand-selection-to-scope.expand",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+down",
        "command": "cursorHome",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+up",
        "command": "cursorEnd",
        "when": "editorTextFocus"
    },
    {
        "key": "alt+ctrl+up",
        "command": "editor.action.insertCursorAbove",
        "when": "editorTextFocus"
    },
    {
        "key": "alt+ctrl+down",
        "command": "editor.action.insertCursorBelow",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+right",
        "command": "cursorEndSelect",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+left",
        "command": "cursorHomeSelect",
        "when": "editorTextFocus"
    },
    {
        "key": "shift+alt+up",
        "command": "commandId",
        "when": "editorTextFocus"
    },
    {
        "key": "shift+alt+down",
        "command": "commandId",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+space",
        "command": "center-editor-window.center",
        "when": "editorTextFocus"
    },
    // {
    //     "key": "ctrl+'",
    //     "command": "expand_region",
    //     "when": "editorTextFocus"
    // },extension.selectSingleQuote
    // {
    //     "key": "ctrl+shift+'",
    //     "command": "undo_expand_region",
    //     "when": "editorTextFocus && editorHasSelection"
    // }
    {
        "key": "ctrl+'",
        "command": "extension.selectDoubleQuote",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+shift+'",
        "command": "extension.selectSingleQuote",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+shift+a",
        "command": "extension.selectInTag",
        "when": "editorTextFocus"
    },
    {
        "key": "shift+alt+left",
        "command": "cursorWordStartLeftSelect",
        "when": "editorTextFocus"
    },
    {
        "key": "shift+alt+right",
        "command": "cursorWordEndRightSelect",
        "when": "editorTextFocus"
    },
    {
        "key": "alt+left",
        "command": "cursorWordStartLeft",
        "when": "editorTextFocus"
    },
    {
        "key": "alt+right",
        "command": "cursorWordEndRight",
        "when": "editorTextFocus"
    },
    {
        "key": "alt+down",
        "command": "-editor.action.moveLinesDownAction",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "alt+up",
        "command": "-editor.action.moveLinesUpAction",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "alt+enter",
        "command": "editor.action.changeAll",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "capslock",
        "command": "workbench.action.closeMessages",
        "when": "globalMessageVisible"
    },
    {
        "key": "capslock",
        "command": "cancelSelection",
        "when": "editorHasSelection && editorTextFocus"
    },
    {
        "key": "capslock",
        "command": "workbench.action.hideInterfaceOverview",
        "when": "interfaceOverviewVisible"
    },
    {
        "key": "capslock",
        "command": "cancelRenameInput",
        "when": "editorFocus && renameInputVisible"
    },
    {
        "key": "capslock",
        "command": "closeAccessibilityHelp",
        "when": "accessibilityHelpWidgetVisible && editorFocus"
    },
    {
        "key": "capslock",
        "command": "closeBreakpointWidget",
        "when": "breakpointWidgetVisible && editorFocus"
    },
    {
        "key": "capslock",
        "command": "closeFindWidget",
        "when": "editorFocus && findWidgetVisible"
    },
    {
        "key": "capslock",
        "command": "closeMarkersNavigation",
        "when": "editorFocus && markersNavigationVisible"
    },
    {
        "key": "capslock",
        "command": "closeParameterHints",
        "when": "editorTextFocus && parameterHintsVisible"
    },
    {
        "key": "capslock",
        "command": "closeReferenceSearch",
        "when": "referenceSearchVisible && !config.editor.stablePeek"
    },
    {
        "key": "capslock",
        "command": "closeReferenceSearchEditor",
        "when": "inReferenceSearchEditor && !config.editor.stablePeek"
    },
    {
        "key": "capslock",
        "command": "workbench.action.closeQuickOpen",
        "when": "inQuickOpen"
    },
    {
        "key": "capslock",
        "command": "leaveSnippet",
        "when": "editorTextFocus && inSnippetMode"
    },
    {
        "key": "capslock",
        "command": "list.clear",
        "when": "listFocus"
    },
    {
        "key": "capslock",
        "command": "removeSecondaryCursors",
        "when": "editorHasMultipleSelections && editorTextFocus"
    },
    {
        "key": "capslock",
        "command": "search.action.cancel",
        "when": "listFocus && searchViewletVisible"
    },
    {
        "key": "capslock",
        "command": "hideSuggestWidget",
        "when": "editorTextFocus && suggestWidgetVisible"
    },
    {
        "key": "capslock",
        "command": "leaveEditorMessage",
        "when": "messageVisible"
    },
    {
        "key": "capslock",
        "command": "closeReplaceInFilesWidget",
        "when": "replaceInputBoxFocus && searchViewletVisible"
    },
    {
        "key": "capslock capslock",
        "command": "workbench.action.exitZenMode",
        "when": "inZenMode"
    }
]