{
    "files.autoSaveDelay": 500,
    "files.autoSave": "onFocusChange",
    "editor.lineHeight": 30,
    "editor.renderLineHighlight": "gutter",
    "editor.renderWhitespace": "none",
    "terminal.integrated.cursorStyle": "line",
    "editor.fontFamily": "Consolas, 'Courier New', monospace",
    "json.format.enable": true,
    "editor.fontSize": 16,
    "workbench.colorCustomizations": {
        "contrastActiveBorder": "#00AA0080",
        "scrollbarSlider.background": "#F5AA0050",
        "scrollbarSlider.hoverBackground": "#F5AA0065",
        "scrollbarSlider.activeBackground": "#F5AA0065",
        "editorCursor.foreground": "#FF0000",
        "sideBar.background": "#1B2B34",
        "editor.background": "#1B2B34",
        "editorLineNumber.foreground": "#777",
        "editor.selectionBackground": "#F5AA0060",
        "editor.selectionHighlightBackground": "#F5AA0030",
        "editor.inactiveSelectionBackground": "#F5AA0030",
        "editor.findMatchBackground": "#F5AA0030",
        "editor.findMatchHighlightBackground": "#F5AA0030",
        "editor.findRangeHighlightBackground": "#F5AA0030",
        "editor.wordHighlightBackground": "#00968850",
        "editor.wordHighlightStrongBackground": "#ffdeaa30",
        "editor.lineHighlightBackground": "#cccccc20",
        "statusBar.background": "#1B2B3400",
        "tab.activeBackground": "#1B2B34",
        "tab.inactiveBackground": "#F5FF0015",
        "tab.border": "#F5CC0030",
        "input.background": "#F5AA0010",
        "input.border": "#F5AA0040",
        "list.activeSelectionBackground": "#F5AA0095",
        "sideBarTitle.foreground": "#F5AA0095",
        "editorWhitespace.foreground": "#fff00030"
    },
    "terminal.integrated.fontSize": 16,
    "editor.fontLigatures": true,
    "workbench.iconTheme": "vscode-great-icons",
    "terminal.integrated.shell.linux": "/bin/zsh",
    "editor.tabSize": 2,
    "editor.insertSpaces": true,
    "editor.wordWrap": "off",
    "editor.formatOnType": true,
    "workbench.welcome.enabled": false,
    "editor.cursorStyle": "line",
    "window.menuBarVisibility": "toggle",
    "explorer.openEditors.visible": 0,
    "workbench.activityBar.visible": false,
    "editor.matchBrackets": true,
    "editor.stablePeek": true,
    "editor.formatOnSave": true,
    "editor.wordWrapColumn": 122,
    "[html]": {
        "editor.formatOnSave": true,
        "editor.formatOnPaste": true,
        "editor.matchBrackets": false
    },
    "workbench.colorTheme": "Hybrid Operator",
    "editor.renderControlCharacters": true,
    "terminal.integrated.fontFamily": "Operator Mono book",
    "workbench.statusBar.visible": false,
    "editor.renderIndentGuides": true,
    "editor.suggestLineHeight": 20,
    "window.zoomLevel": 0,
    "previewServer.port": 4200,
    "search.exclude": {
        "**/.git": true,
        "**/node_modules": true,
        "**/.history": true,
        "**/bower_components": true,
        "**/tmp": true
    },
    "editor.minimap.enabled": false
}
